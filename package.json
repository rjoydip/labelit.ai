{
    "name": "labelit.ai",
    "version": "0.0.1",
    "packageManager": "bun@1.1.3",
    "workspaces": [
        "packages/*",
        "integrations/*"
    ],
    "scripts": {
        "prepare": "husky",
        "dev": "turbo run dev",
        "build": "turbo run build",
        "lint": "turbo run lint",
        "test": "turbo run test",
        "typecheck": "turbo run typecheck",
        "update": "turbo run update -- --ignore-scripts --no-progress --no-summary"
    },
    "devDependencies": {
        "husky": "^9.1.7",
        "turbo": "^2.4.4"
    }
}